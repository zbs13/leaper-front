{
    "env": {
        "browser": true,
        "es2021": true,
        "react-native/react-native": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
         } 
    },
    "rules": {
        "no-useless-escape": 0,
        "no-unused-vars": 0,
        "no-extra-semi": 0,
        "no-case-declarations": 0,
        "no-undef": 0,
        "no-dupe-keys": 0,
        "no-global-assign": 0,
        "no-unreachable": 0,
        "no-empty": 0
    },
    "plugins": [
        "react",
        "react-native"
      ]
}
